{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./src/@types",
      "./node_modules/@types",
      "./src/gatsby-types.d.ts"
    ],
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@features": ["src/features"],
      "@features/*": ["src/features/*"],
      "@contexts": ["src/contexts"],
      "@contexts/*": ["src/contexts/*"],
      "@mixins": ["src/mixins"],
      "@mixins/*": ["src/mixins/*"],
      "@hooks/*": ["src/hooks/*"],
      "@hooks": ["src/hooks"],
      "@helper/*": ["src/helper/*"],
      "@helper": ["src/helper"],
      "@pages/*": ["src/pages/*"],
      "@pages": ["src/pages"],
      "@assets/*": ["src/assets/*"],
      "@assets": ["src/assets"],
      "@views/*": ["src/views/*"],
      "@views": ["src/views"],
      "@generated/*": ["src/generated/*"],
      "@generated": ["src/generated"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "./src/**/*",
    "./gatsby-node.ts",
    "./plugins/**/*"
  ],
  "exclude": ["node_modules", "**/generated/*"]
}
